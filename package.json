  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "ts-node --project tsconfig.node.json scripts/apply-migrations.ts",
    "db:types": "ts-node --project tsconfig.node.json scripts/generate-types.ts",
    "db:test": "ts-node --project tsconfig.node.json scripts/test-database-functions.ts",
    "db:setup": "npm run db:migrate && npm run db:types",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:ui": "NODE_OPTIONS=--experimental-vm-modules jest --ui",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern='^(?!.*\\.integration\\.test\\.|.*\\.e2e\\.test\\.).*\\.test\\.(ts|tsx)$'",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern='.*\\.integration\\.test\\.(ts|tsx)$'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:security": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern='tests/security/.*\\.test\\.(ts|tsx)$'",
    "test:api": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern='tests/api/.*\\.test\\.(ts|tsx)$'",
    "test:accessibility": "playwright test e2e/accessibility.spec.ts",
    "test:all": "bash ./scripts/run-tests.sh all",
    "api:docs": "redoc-cli serve docs/openapi.yaml -p 8080",
    "generate-types": "ts-node --project tsconfig.node.json scripts/generate-types.ts",
    "security-check": "npx depcheck && npx eslint . --max-warnings=0 --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
